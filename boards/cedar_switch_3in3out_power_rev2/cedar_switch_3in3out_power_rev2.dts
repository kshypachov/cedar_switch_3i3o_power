&clk_lsi {
	status = "okay";
};

&clk_lse {
	clock-frequency = <32768>;
	status = "okay";
};

&clk_hse {
//	hse-bypass;
	clock-frequency = <DT_FREQ_M(8)>; /* STLink 8MHz clock */
	status = "okay";
};

&pll {
	div-m = <4>;
	mul-n = <100>;
	div-p = <2>;
	div-q = <8>;
	clocks = <&clk_hse>;
	status = "okay";
};

&plli2s {
	status = "okay";
	div-m = <4>;
	mul-n = <96>;
	div-q = <4>;
	div-r = <6>;
};

&rcc {
	status = "okay";
	clocks = <&pll>;
	clock-frequency = <DT_FREQ_M(100)>;
	ahb-prescaler = <1>;
	apb1-prescaler = <2>;
	apb2-prescaler = <1>;
};


&clk48 {
	compatible = "st,stm32-clock-mux";
	clocks = <&rcc STM32_SRC_PLLI2S_Q CK48M_SEL(1)>;
	status = "okay";
};

&rtc {
	clocks = <&rcc STM32_SRC_LSE RTC_SEL(1)>;
	status = "okay";
};

&rng {
	status = "okay";
};

